    "angular2-moment": "^1.3.3",
    "angular2-notifications": "^0.7.4",
    "bootstrap": "^3.3.7",
    "chart": "^0.1.2",
    "core-js": "^2.4.1",
    "datamaps": "^0.5.8",
    "font-awesome": "^4.7.0",
    "horton-style": "git+ssh://git@github.com/hortonworks/horton-style.git#dist",
    "immutable": "^3.8.1",
