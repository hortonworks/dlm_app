{
  "name": "dlm-web",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=7.0.0"
  },
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "dev": "ng serve --host 0.0.0.0 --proxy-config proxy.conf.dev.json",
    "prod": "ng serve --target=production --host 0.0.0.0 --proxy-config proxy.conf.prod.json",
    "build": "ng build --target=production --base-href \"/dlm/\"",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "test:watch": "ng test",
    "test": "ng test --watch false",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "e2edebug": "protractor debug protractor.conf.js --elementExplorer",
    "dev:hmr": "ng serve --hmr -e=dev-hmr --host 0.0.0.0 --proxy-config proxy.conf.dev.json",
    "dev:staging": "npx ng serve --hmr -e=dev-hmr --host 0.0.0.0 --proxy-config proxy.conf.staging.json -p 4201",
    "alltests": "yarn run build && yarn run lint && yarn run test",
    "report:licenses": "nlf"
  },
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@ngrx/effects": "^4.0.0",
    "@ngrx/router-store": "^4.0.0",
    "@ngrx/store": "^4.0.0",
    "@ngx-translate/core": "~8.0.0",
    "@ngx-translate/http-loader": "~2.0.0",
    "@swimlane/ngx-datatable": "https://github.com/geksilla/ngx-datatable.git#fix/save-expansions-state-on-updates",
    "angular2-moment": "^1.7.0",
    "angular2-notifications": "0.9",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "horton-style": "git+ssh://git@github.com/hortonworks/horton-style.git#dist",
    "immutable": "^3.8.2",
    "jquery": "^3.1.1",
    "leaflet": "1.2.0",
    "leaflet-curve": "^1.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.0",
    "moment": "^2.19.1",
    "moment-timezone": "^0.5.13",
    "mydatepicker": "^2.4.0",
    "ng2-charts": "^1.6.0",
    "ng2-select": "~2.0.0",
    "ngrx-store-logger": "^0.2.0",
    "ngx-bootstrap": "^2.0.0-beta.7",
    "ngx-clipboard": "^8.1.3",
    "ngx-ui-switch": "^1.4.9",
    "reselect": "^3.0.1",
    "rxjs": "5.5.6",
    "ts-helpers": "^1.1.1",
    "uuid": "^3.1.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/d3-tip": "^3.5.5",
    "@types/file-saver": "^1.3.0",
    "@types/jasmine": "2.6.1",
    "@types/jquery": "^2.0.40",
    "@types/leaflet": "^1.0.60",
    "@types/leaflet-curve": "^0.0.30",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.1",
    "@types/node": "^7.0.12",
    "@types/nvd3": "^1.8.38",
    "codelyzer": "^4.0.0",
    "jasmine-core": "2.6.1",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-istanbul": "^0.2.1",
    "nlf": "^2.0.0",
    "protractor": "5.1.1",
    "ts-node": "2.0.0",
    "tslint": "^5.0.0",
    "typescript": "~2.4.2"
  }
}
