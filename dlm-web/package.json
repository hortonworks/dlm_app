{
  "name": "dlm-web",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=9.3.0"
  },
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "dev": "ng serve --host 0.0.0.0 --proxy-config proxy.conf.dev.json",
    "prod": "ng serve --prod --host 0.0.0.0 --proxy-config proxy.conf.prod.json",
    "build": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --source-map --base-href \"/dlm/\" --build-optimizer=false",
    "lint": "ng lint dlm-web --type-check",
    "lint:fix": "ng lint dlm-web --type-check --fix",
    "test:watch": "ng test --source-map=false",
    "test:ci": "ng test --source-map=false --watch false --browsers=\"PhantomJS\"",
    "test": "ng test --source-map=false --watch false",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "e2edebug": "protractor debug protractor.conf.js --elementExplorer",
    "dev:hmr": "ng serve --hmr --configuration=dev-hmr --host 0.0.0.0 --proxy-config proxy.conf.dev.json",
    "dev:staging": "ng serve --hmr --configuration=dev-hmr --host 0.0.0.0 --proxy-config proxy.conf.staging.json --port 4201",
    "alltests": "yarn run build && yarn run lint && yarn run test",
    "report:licenses": "nlf"
  },
  "dependencies": {
    "@angular/animations": "6.0.9",
    "@angular/cli": "6.0.8",
    "@angular/common": "6.0.9",
    "@angular/compiler": "6.0.9",
    "@angular/core": "6.0.9",
    "@angular/forms": "6.0.9",
    "@angular/http": "6.0.9",
    "@angular/platform-browser": "6.0.9",
    "@angular/platform-browser-dynamic": "6.0.9",
    "@angular/router": "6.0.9",
    "@ngrx/effects": "^6.0.0",
    "@ngrx/entity": "^6.1.0",
    "@ngrx/router-store": "^6.0.0",
    "@ngrx/store": "^6.0.0",
    "@ngx-translate/core": "~10.0.2",
    "@ngx-translate/http-loader": "~3.0.1",
    "@swimlane/ngx-datatable": "https://github.com/geksilla/ngx-datatable.git#fix/save-expansions-state-on-updates-a6",
    "angular2-notifications": "1.0.2",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.7",
    "dps-apps": "git+ssh://git@github.com/hortonworks/dps-apps.git#0.1.4-alpha.16",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "horton-style": "git+ssh://git@github.com/hortonworks/horton-style.git#dist",
    "immutable": "^3.8.2",
    "jquery": "^3.1.1",
    "leaflet": "1.2.0",
    "leaflet-curve": "^1.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.13",
    "mydatepicker": "^2.4.0",
    "ng2-charts": "^1.6.0",
    "ng2-select": "~2.0.0",
    "ngrx-store-logger": "^0.2.0",
    "ngx-bootstrap": "3.0.1",
    "ngx-clipboard": "^11.1.1",
    "ngx-moment": "^3.1.0",
    "reselect": "^3.0.1",
    "rxjs": "^6.2.2",
    "ts-helpers": "^1.1.1",
    "uuid": "^3.1.0",
    "whatwg-fetch": "^2.0.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/schematics": "0.6.8",
    "@angular/compiler-cli": "6.0.9",
    "@angularclass/hmr": "^2.1.3",
    "@types/d3-tip": "^3.5.5",
    "@types/file-saver": "^1.3.0",
    "@types/jasmine": "2.6.1",
    "@types/jquery": "^2.0.40",
    "@types/leaflet": "^1.0.60",
    "@types/leaflet-curve": "^0.1.1",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.1",
    "@types/node": "^7.0.12",
    "@types/nvd3": "^1.8.38",
    "codelyzer": "^4.4.2",
    "jasmine-core": "2.6.1",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-istanbul": "^0.6.0",
    "nlf": "^2.0.0",
    "protractor": "^5.4.0",
    "puppeteer": "^1.6.1",
    "ts-node": "2.0.0",
    "tslint": "^5.11.0",
    "typescript": "2.7.2"
  }
}
